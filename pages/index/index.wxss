@import "../nearbyEquipment/nearbyEquipment.wxss";

page {
    background: #fff;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.container {
    padding: 40rpx;
    box-sizing: border-box;
}

.dls_swiper {
    height: 240rpx;
    border-radius: 20px;
    margin: 0 18rpx 12rpx;
}

.slide-image {
    height: 100%;
    border-radius: 20px;
    width: 100%;
}

.dls_operate {
    display: flex;
    padding: 40rpx 0 0;
    width: 100%;
    height: 230rpx;
    box-sizing: border-box;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: center;
    align-items: center;
}

.iterm_operate {
    display: inline-flex;
    height: 100%;
    width: 25%;
    flex-shrink: 1;
    box-sizing: border-box;
    flex-direction: column;
}

.icon_view {
    display: inline-flex;
    width: 100%;
    justify-content: center;
    align-items: center;
    padding-bottom: 20rpx;
}

.opera_txt {
    width: 100%;
    margin: 0 auto;
    font-size: 12px;
    color: #333;
    text-align: center;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.img_bgc {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    box-sizing: border-box;
    display: flex;
    justify-content: center;
    align-items: center;
}

.icon-image {
    width: 50rpx;
    height: 50rpx;
}

.partthree {
    width: 100%;
    padding-top: 20rpx;
    background-color: #f4f5f6;
}

.list_group {
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: space-between;
    width: 100%;
    height: 234rpx;
    box-sizing: border-box;
    padding: 20px 0;
    background-color: #fff;
}

.group_itemz .iterm_tit,.group_itemz .iterm_num {
    display: block;
}

.iterm_num {
    font-size: 24px;
    color: #121212;
}

.iterm_tit {
    color: #666;
    font-size: 14px;
    margin-bottom: 20rpx;
}

.enterto {
    display: inline-block;
    padding: 8rpx 40rpx 8rpx 20rpx;
    color: #a09e9e;
    border: 1px solid currentColor;
    border-radius: 40rpx;
    font-size: 12px;
}

.group_item {
    display: inline-flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
}

.group_item .weui-cell__ft_in-access:after {
    width: 6px;
    height: 6px;
    margin-top: -4px;
    right: 20rpx;
    border-width: 1px 1px 0 0;
}

.gotoWallet {
    display: inline-block;
    padding: 8rpx 20rpx;
    color: #a09e9e;
    border: 1px solid currentColor;
    border-radius: 40rpx;
    font-size: 12px;
}

.nogotoWallet {
    display: none;
}

.bgimg {
    position: relative;
}

.layout {
    position: absolute;
    height: 234rpx;
    width: 100%;
    top: 0;
    left: 0;
}

.list_group {
    display: flex;
    flex-direction: row;
    flex-wrap: norap;
    justify-content: space-between;
    align-items: center;
}

.items_list {
    display: inline-flex;
    width: 49%;
    flex-direction: column;
    flex-wrap: norap;
    text-align: center;
    padding-left: 100rpx;
}

.arrows {
    color: #666666;
    font-size: 12px;
    display: inline-flex;
    justify-content: flex-start;
    align-items: center;
}

.img_arrows {
    width: 16rpx;
    height: 26rpx;
    margin-left: 14rpx;
}

.cash_num::after {
    content: "积分";
    display: inline-block;
    font-size: 15px;
    color: #121212;
    margin-left: 10rpx;
}

.before_box {
    height: 72rpx;
    width: 1px;
    background-color: #CCCCCC;
}

.items_list .iterm_num {
    text-align: left;
}

.nearby_wrap {
    width: 100%;
    height: 234rpx;
    position: absolute;
    top: 0;
    left: 0;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    padding: 20rpx 0 30rpx;
}

.nearby_item {
    width: 100%;
    padding-left: 100rpx;
}

.dls_btext {
    color: #121212;
    font-size: 16px;
}

.dls_bicon {
    width: 14px;
    height: 17px;
    margin-right: 12rpx;
}

.nearby_itemt {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding-right: 50rpx;
}

.nearby_items {
    display: inline-flex;
    justify-content: center;
    align-items: center;
}

.dls_bgimg {
    width: 154rpx;
    height: 64rpx;
    color: #FFFFFF;
    font-size: 12px;
    border-radius: 64rpx;
    background: url('data:image/png;base64,') no-repeat center center;
}

.dls_sicon {
    width: 28rpx;
    height: 28rpx;
    margin-right: 12rpx;
}

.dls_stext {
    color: #EC7301;
    font-size: 12px;
}

.nearby_item3 {
    width: 100%;
    padding-left: 100rpx;
    display: flex;
    align-items: center;
}